{
  "name": "modular-snippets",
  "version": "0.1.3",
  "description": "A modular solution to snippets in Atom.",
  "keywords": [
    "snippets",
    "modular",
    "config"
  ],
  "homepage": "https://atom.io/packages/modular-snippets#readme",
  "repository": "danielbayley/atom-modular-snippets",
  "bugs": "https://github.com/danielbayley/atom-modular-snippets/issues",
  "author": "Daniel Bayley <daniel.bayley@me.com> (https://github.com/danielbayley)",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "glob": "^7.0.5",
    "require-cson": "^0.1.0"
  },
  "scripts": {
    "preinstall": "mkdir -p \"${ATOM_HOME:-$HOME/.atom}\"/snippets"
  },
  "engines": {
    "atom": "*"
  },
  "providedServices": {
    "modular-snippets": {
      "description": "Load any snippets object, file or folder.",
